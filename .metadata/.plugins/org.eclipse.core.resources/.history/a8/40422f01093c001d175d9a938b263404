package varaibles;

public class Main {
	public static void main(String[] args) {
		int ogrenciiSayisi = 12;
		String mesaj ="Öğrenci sayısı";
		System.out.println(mesaj+ ogrenciiSayisi);
		System.out.println(mesaj+ ogrenciiSayisi);
		System.out.println("Öğrenci sayım : "+ ogrenciiSayisi);
		System.out.println("Öğrenci sayım : "+ ogrenciiSayisi);
		
		
		
		
	}

}
